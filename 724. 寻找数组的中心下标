public class Solution {
    public int PivotIndex(int[] nums) {
        int n=nums.Length;
        int answer=0;
        for(int i=0;i<n;i++){
            int a1=0,b1=0;
            for(int a=0;a<i;a++){
                a1+=nums[a];
            }
            for(int b=n-1;b>i;b--){
                b1+=nums[b];
            }
            if(a1==b1){
                answer=i;
                return answer;
            }
        }
        answer=-1;
        return answer;
    }
}
